<h3>Add Song</h3>
<div class="row">
    <form action="/songs" method="POST" class="col s12">
        <div class="row">
            <div class="input-field">
                <input type="text" id="title" name="title">
                <label for="title">Song Title</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="artist" name="artist">
                <label for="artist">Artist</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="genre" name="genre">
                <label for="genre">Genre</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="mood" name="mood">
                <label for="mood">Mood</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="theme" name="theme">
                <label for="theme">Theme</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="coWriter" name="coWriter">
                <label for="coWriter">Co-Writer</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="writerSplit" name="writerSplit">
                <label for="writerSplit">Writer Splits</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="pro" name="pro">
                <label for="pro">PRO Infomation</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="songLink" name="songLink">
                <label for="songLink">Recording Link</label>
            </div>
        </div>
